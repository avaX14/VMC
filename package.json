{
  "homepage": "http://avaX14.github.io/VMC",
  "name": "react-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build && cp build/index.html build/404.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "generate": "plop --plopfile generators/index.js",
    "extract-intl": "NODE_ENV=development extract-messages -l=en -o src/translations -d en --flat 'src/**/!(*.test).js'",
    "prettify": "prettier --write"
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "^1.3.0",
    "@material-ui/core": "^4.5.1",
    "@sentry/browser": "^5.7.0",
    "animate.css": "^3.7.2",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.0",
    "connected-react-router": "^6.5.2",
    "deepstream.io-client-js": "^2.3.4",
    "formik": "^1.5.8",
    "gh-pages": "^3.1.0",
    "husky": "^3.0.9",
    "immer": "^4.0.1",
    "intl": "^1.2.5",
    "invariant": "^2.2.4",
    "jwt-decode": "^2.2.0",
    "leaflet": "^1.6.0",
    "lint-staged": "^9.4.2",
    "lodash": "^4.17.15",
    "node-sass": "^4.14.1",
    "notistack": "^0.9.4",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-dom": "^16.10.1",
    "react-elastic-carousel": "^0.6.4",
    "react-facebook-login": "^4.1.1",
    "react-flags-select": "^1.1.13",
    "react-google-login": "^5.0.5",
    "react-helmet-async": "^1.0.3",
    "react-hook-form": "^6.0.1",
    "react-intl": "^3.6.1",
    "react-leaflet": "^2.7.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
    "react-spring": "^8.0.27",
    "react-svg-map": "^2.1.2",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.8",
    "extract-react-intl-messages": "^2.3.3",
    "plop": "^2.4.0",
    "plop-prettier": "^2.0.1",
    "source-map-explorer": "^2.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
